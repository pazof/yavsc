        },
        "licenseUrl": "https://github.com/pazof/yavsc/blob/vnext/LICENSE",
        "requireLicenseAcceptance": true,
        "owners": [
            "Paul Schneider <paul@pschneider.fr>"
        ],
        "summary": "Yet another very small company",
        "projectUrl": "http://yavsc.pschneider.fr",
        "tags": [
            "Blog",
            "Blog",
            "PoS",
            "Chat"
        ]
