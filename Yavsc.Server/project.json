        },
        "net451": {
            "frameworkAssemblies": {
                "System.ComponentModel.DataAnnotations": "4.0.0",
                "System.Json": "4.0.0",
                "System.Net": "4.0.0.0",
                "System.Net.Http": "4.0.0",
                "System.Xml": "4.0.0",
                "System": "4.0.0"
