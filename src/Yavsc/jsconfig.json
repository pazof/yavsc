{
	// See http://go.microsoft.com/fwlink/?LinkId=759670
	// for the documentation about the jsconfig.json format
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6"
	},
	"include": [
		"wwwroot/js/**/*"
		],
	"exclude": [
		"bin",
		"src",
		"Resources",
		"Properties",
		"contrib",
		"node_modules",
		"bower_components",
		"jspm_packages",
		"tmp",
		"temp"
	]
}
