{
  "version": "1.0.6-*",
  "description": "encore une tr√®s petite entreprise",
  "authors": [
    "Paul Schneider <paul@pschneider.fr>"
  ],
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "https://github.com/pazof/yavsc"
    },
    "licenseUrl": "https://github.com/pazof/yavsc/blob/vnext/LICENSE",
    "requireLicenseAcceptance": true,
    "owners": [
      "Paul Schneider <paul@pschneider.fr>"
    ],
    "summary": "Yet another very small company",
    "projectUrl": "http://yavsc.pschneider.fr",
    "tags": [
      "Blog",
      "PoS",
      "Chat"
    ]
  },
  "userSecretsId": "aspnet5-YavscWeb-a0dadd21-2ced-43d3-96f9-7e504345102f",
  "exclude": [
    "bin",
    "wwwroot",
    "node_modules",
    "bower_components",
    "contrib"
  ],
  "buildOptions": {
    "debugType": "full",
    "emitEntryPoint": true,
    "outputName": "Yavsc",
    "compile": {
      "include": "*.cs",
      "exclude": [
          "wwwroot",
          "node_modules",
          "bower_components",
          "contrib"
      ]
    },
    "embed": [
      "Resources/**/*.resx"
    ]
  },
  "configurations": {
    "Debug": {
      "compilationOptions": {
        "emitEntryPoint": true,
        "define": [
          "DEBUG",
          "TRACE"
        ],
        "optimize": false,
        "debugType": "full",
        "platform": "anycpu"
      }
    },
    "Release": {
      "compilationOptions": {
        "define": [
          "RELEASE",
          "TRACE"
        ],
        "optimize": true
      }
    }
  },
  "webroot": "wwwroot",
  "tooling": {
    "defaultNamespace": "Yavsc"
  },
